.contents
  .contents__header
    = link_to "#", class: "contents__header__name" do
      = icon('fas', 'swimmer')
      宮古島
    %ul.contents__header__btn
      %li= link_to "新規登録", "#", class: "contents__header__btn__a" 
      %li= link_to "ログイン", "#", class: "contents__header__btn__b" 
  .container_top
    .container_top__info
      %hi Let's have a good time 
      %h1 in
      %hi Miyako Island
      %h3 ~一生に一度の思い出を~
      %p Do you wanna join us?
    .container_top__btn
      = link_to "#", class: "container-btn container_top__btn_user" do
        = icon('fas', 'user')
        新規登録
      = link_to "#", class: "container-btn container_top__btn_facebook" do
        = icon('fab', 'facebook')
        Facebookで登録
      = link_to "#", class: "container-btn container_top__btn_twitter" do
        = icon('fab', 'twitter')
        Twitterで登録
  .container_middle
    .container_middle_info
      %p ダイビング
      %image{:src => "data:image/jpeg;base64,"}
    .container_middle_info
      %p 星座観賞
      %image{:src => "https://encrypted-tbn0.gstatic.com/images?q=tbn%3AANd9GcSafyz3P-VU47-nyRlo2vDc1SjP8WUUGZiAR0Kg6JKzRmMBpEjS"}
    .container_middle_info
      %p 宮古グルメ
