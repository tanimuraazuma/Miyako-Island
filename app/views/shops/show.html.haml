.wrapper
  .shops-header
    = link_to root_path, class: "shops-header__name" do
      = icon('fas', 'swimmer')
      宮古島
    .shops-header__btn  
      %ul.shops-header__btn__ul
        %li= link_to "Diving", "#", class: "shops-header__btn__ul__li"
        %li= link_to "Food", "#", class: "shops-header__btn__ul__li"
        %li= link_to "Star", "#", class: "shops-header__btn__ul__li"
  .signup#signup-modal
    .signup__info
      #signup-close
        %i.fa.fa-2x.fa-times
      #signup-form
        %h2 名前
        = form_with url: "#", method: :post, local: true do |f|
          = f.text_field :name, placeholder: "名前", class: "form-control"
          = f.email_field :email, placeholder: "Eメールアドレス", class: "form-control"
          #submit-btn 予約
  .shops_info
    %ul.shops_info__btn.delete#a_a
      %li.ppp
        = link_to "大物(海亀、エイ、イルカ)", divings_path, class: "shops_info__btn__a"
      %li.ppp
        = link_to "地形ダイビング", index_b_divings_path, class: "shops_info__btn__a"
      %li.ppp
        = link_to "インスタ映え", index_c_divings_path, class: "shops_info__btn__a"
      %li.ppp
        = link_to "未経験者ok", "#", class: "shops_info__btn__a"
      %li.ppp
        = link_to "値段で探す", "#", class: "shops_info__btn__a"
    %ul.shops_info__btn.delete#b_b
      %li.ppp
        = link_to "宮古牛", foods_path, class: "shops_info__btn__a"
      %li.ppp
        = link_to "海鮮", index_b_foods_path, class: "shops_info__btn__a"
      %li.ppp
        = link_to "宮古そば", index_c_foods_path, class: "shops_info__btn__a"
      %li.ppp
        = link_to "レビュー評価", "#", class: "shops_info__btn__a"
      %li.ppp
        = link_to "値段で探す", "#", class: "shops_info__btn__a"
    %ul.shops_info__btn.delete#c_c
      %li.ppp
        = link_to "オリオン座", stars_path, class: "shops_info__btn__a"
      %li.ppp
        = link_to "夏の大三角形", "#", class: "shops_info__btn__a"
      %li.ppp
        = link_to "北斗七星", "#", class: "shops_info__btn__a"
      %li.ppp
        = link_to "南十字星", "#", class: "shops_info__btn__a"
      %li.ppp
        = link_to "値段で探す", "#", class: "shops_info__btn__a"
  .shops
    .shops__info
      .shops__info__content
        %h1.shops__info__content__name #{@shop.name}
        %h2 #{@shop.description}
        .change-btn-wrapper
          .change-btn.prev-btn ← 前へ
          .change-btn.next-btn 次へ →
        %ul.slides
          %li.slide.active
            = image_tag("#{@shop.image.url}", width: "275", height: "183")
          %li.slide 
            %image{:src => "data:image/jpeg;base64,"}
          %li.slide 
            %image{:src => "data:image/jpeg;base64,"}
          %li.slide 
            %image{:src => "data:image/jpeg;base64,"}
        .index-btn-wrapper
          .index-btn 1
          .index-btn 2
          .index-btn 3
          .index-btn 4 
        .infomation
          %h1 ¥#{@shop.price}~
          = link_to "予約する", "#", class: "reservation"
